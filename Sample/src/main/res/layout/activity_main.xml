<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.MainActivity">

    <Button
        android:id="@+id/bt_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="test" />

    <com.chinapex.android.datacollect.testAop.CabbageButton
        android:id="@+id/cb_jar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bt_test"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="我是jar包的button" />

    <Button
        android:id="@+id/bt_2second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cb_jar"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="跳转到第二个activity" />

</RelativeLayout>